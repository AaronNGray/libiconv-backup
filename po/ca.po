#: src/iconv.c:80
msgid "Usage: iconv [-c] [-s] [-f fromcode] [-t tocode] [file ...]"
msgstr ""
"Forma d'ús: iconv [-c] [-s] [-f CODIF_ORIG] [-t CODIF_DEST] [FITXER ...]"

#: src/iconv.c:82
msgid "or:    iconv -l"
